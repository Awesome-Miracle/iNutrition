.cart-box{
    padding-bottom: 10rpx;
}
.cart-list{
    position: relative;
    padding: 20rpx 20rpx 20rpx 150rpx;
    height: 100rpx;
    border-bottom: 1rpx solid #e9e9e9;
}
.cart-list .cart-thumb{
    border: lightgrey;
    border-style: solid;
    border-width: 1px;
    border-bottom-left-radius: 10px;
    border-top-left-radius: 10px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    position: absolute;
    top: 20rpx;
    left: 30rpx;
    width: 100rpx;
    height: 100rpx;
}
.cart-list .cart-pro-name{
    display: inline-block;
    width: 300rpx;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
}
.cart-list .cart-pro-info{
    color:#999;
    font-size: 15px;
    display: inline-block;
    width: 300rpx;
    height: 50rpx;
    line-height: 50rpx;
    overflow: hidden;
}
.cart-list .cart-pro-score{
    border:lightgray;
    border-style: solid;
    border-width: 1px;
    border-bottom-left-radius: 10px;
    border-top-left-radius: 10px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    position: absolute;
    font-size: 30px;
    top: 10rpx;
    right: 20rpx;
    overflow: hidden;
    text-align: center;
    width: 100rpx;
    height: 90rpx;
}
.cart-list .cart-pro-eva{
    color:#999;
    position: absolute;
    font-size: 10px;
    bottom: 5rpx;
    right: 20rpx;
    overflow: hidden;
    text-align: center;
    width: 100rpx;
}
.cart-del{
    position: absolute;
    right: 10rpx;
    bottom: 25rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 44rpx;
}
.cart-title{
    bottom: 0;
    left: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    padding:0 100rpx 0 30rpx;
    box-sizing: border-box;
    background: #e9e9e9;
    color: #000;
}

.cart-no-data{
    padding:40rpx 0;
    color: #999;
    text-align: center;
}

.weui-search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #efeff4;
  border-top: 1rpx solid #d7d6dc;
  border-bottom: 1rpx solid #d7d6dc;
}

.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  background: #fff;
  border: 1rpx solid #e6e6ea;
}

.weui-icon-search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}

.weui-search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.weui-search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
  color:#999;
}

.thumb{
    border: lightgrey;
    border-style: solid;
    border-width: 1px;
    border-bottom-left-radius: 10px;
    border-top-left-radius: 10px;
    border-bottom-right-radius: 10px;
    border-top-right-radius: 10px;
    position: absolute;
    top: 20rpx;
    left: 30rpx;
    width: 100rpx;
    height: 100rpx;
}